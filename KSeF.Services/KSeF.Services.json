{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "KSeF.Services.Indicator": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "KSeF.Services.HttpRequestsObserver": "Trace",
      "Microsoft.Extensions.Http.DefaultHttpClientFactory": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information",
        "KSeF.Services.HttpRequestsObserver": "Trace"
      },
      "FormatterName": "simple",
      "FormatterOptions": {
        "IncludeScopes": false
      }
    }
  },
  "EOLReplacements": {
    "CR": 3,
    "LF": 2
  }
}
